import Model from '../Model'

// <%- remark%>
export class <%- tableNameUpperCase %>Model extends Model {
  constructor({<%- propStr %>}) {
    super()
    <% for (var i=0; i<column.length; i++) { %>
    this.<%- column[i].val %>= {
    val: <%- column[i].val%>,
    label: '<%- column[i].label%>',
    status: true,
    type: '<%- column[i].type%>'
    } // <%- column[i].label%>
    <% } %>
  }
}

export default function create<%- tableNameUpperCase %>(<%- tableNameUpperCase %> = {}) {
  return new <%- tableNameUpperCase %>Model({
  <%for(let i = 0; i < prop.length; i++){ -%>
  <% if (i !== prop.length - 1){ %>
  <%- prop[i]%>: <%- tableNameUpperCase %>.<%- prop[i] -%>,
  <% } -%>
  <% if (i === prop.length - 1) { %>
  <%- prop[i]%>: <%- tableNameUpperCase %>.<%- prop[i] -%>
  <% } -%>
  <%}%>
  })
}
